<?xml version="1.0" encoding="utf-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
xmlns="http://java.sun.com/xml/ns/javaee"
xmlns:web="http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
xsi:schemaLocation="http://java.sun.com/xml/ns/javaee
http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" version="2.5">
	
	<servlet>
		<servlet-name>findServlet</servlet-name>
		<servlet-class>newsReader.Find</servlet-class>
	</servlet>
	<servlet-mapping>
	 <servlet-name>findServlet</servlet-name>
	 <url-pattern>/find</url-pattern>
	</servlet-mapping>
	
	<servlet>
	 <servlet-name>ProcessContentServlet</servlet-name>
	 <servlet-class>newsReader.ProcessContent</servlet-class>
	</servlet>>
	<servlet-mapping>
	<servlet-name>ProcessContentServlet</servlet-name>
	<url-pattern>/processContent</url-pattern>
	</servlet-mapping>
	
	<welcome-file-list>
		<welcome-file>index.html</welcome-file>
	</welcome-file-list>
	
</web-app>

